#home-container
  width: 100%
  height: max-content
  padding: 1rem 0

#home-content
  width: 100%
  max-width: var(--meow-main-width)
  margin: 0 auto
  display: flex
  flex-direction: row

#home-sidebar
  width: 32%
  height: 800px
  background-color: mistyrose
  &>div
    border-radius: 10px

#home-main
  width: 100%
  height: 1000px
  margin: 0 .625rem
  background-color: var(--background-color-transparent)
  &>div
    border-radius: 10px